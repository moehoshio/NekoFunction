{
  "name": "neko-function",
  "version": "1.0.0",
  "description": "A comprehensive modern C++ utility library that provides practical functions for common programming tasks.",
  "homepage": "https://github.com/moehoshio/NekoFunction",
  "license": "MIT OR Apache-2.0",
  "dependencies": [
    "neko-schema",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "archive": {
      "description": "Build with archive support",
      "dependencies": [
        {
            "name": "minizip-ng",
            "version>=": "4.0"
        }
      ]
    },
    "hash": {
      "description": "Build with hash support",
      "dependencies": [
        {
          "name": "openssl",
          "version>=": "3.0"
        }
      ]
    }
  }
}
